<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Module_limit</fullName>
    <active>true</active>
    <errorConditionFormula>OR(No_of_Modules__c &lt;= 0,No_of_Modules__c &gt; 12)</errorConditionFormula>
    <errorDisplayField>No_of_Modules__c</errorDisplayField>
    <errorMessage>The No. of modules should be between 1 to 12.</errorMessage>
</ValidationRule>
